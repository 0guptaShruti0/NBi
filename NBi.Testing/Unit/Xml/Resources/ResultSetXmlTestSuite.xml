<?xml version="1.0" encoding="utf-8" ?>
<testSuite name="The Query TestSuite" xmlns="http://NBi/TestSuite">
	<test name="My first test case" uid="0001">
		<system-under-test>
			<execution>
				<query name="Select first departments">
					select top 1 [Name] from [HumanResources].[Department] order by Name
				</query>
			</execution>
		</system-under-test>
		<assert>
			<equalTo>
				<resultSet>
					<row>						
						<cell>Document Control</cell>
					</row>
				</resultSet>
			</equalTo>
		</assert>
	</test>
</testSuite>
